  },
  "dependencies": {
    "@popperjs/core": "^2.11.8",
    "bootstrap": "^5.3.3",
    "dashicons": "^0.0.2",
    "glider-js": "^1.7.8",
    "glightbox": "^3.2.0",
    "sass": "^1.57.1"
  }
}
